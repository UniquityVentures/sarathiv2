<odoo>
	<data>
		<record id="group_student_user" model="res.groups">
			<field name="name">Student</field>
			<field name="category_id" ref="base.module_category_students"/>
			<field name="implied_ids" eval="[(6, 0, [ref('base.group_user')])]"/>
			<field name="comment">The user will be able to view information related to his own courses and batches</field>
		</record>
		<record id="group_teacher_user" model="res.groups">
			<field name="name">Teacher</field>
			<field name="category_id" ref="base.module_category_teachers"/>
			<field name="implied_ids" eval="[(6, 0, [ref('base.group_user')])]"/>
			<field name="comment">The user will be able to view and edit information related to his own courses and batches</field>
		</record>
	</data>
</odoo>
